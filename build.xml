<?xml version="1.0" encoding="utf-8"?>
<project name="Java1.8 Build" basedir=".">
	<target name="build">
		<echo message="Creating EAR file!"/>
		<delete dir="./bin" />
		<mkdir dir="./bin" />
		<javac srcdir="./src" destdir="./bin" debug="true" debuglevel="vars,lines,source" 	includes="**/*.java" encoding="UTF-8">
		</javac>
	</target>
</project>